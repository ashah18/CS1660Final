@model List<vwSavedModel>

<head>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
        integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
        integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
</head>
<style>
    tbody,
    td,
    tfoot,
    th,
    thead,
    tr {
        border-width: 1px;
        text-align: center;
    }

    thead {
        font-weight: bold;
    }

    .backButton {}
</style>

<div>
    <header>
        <h1 class="text-center">Saved Models</h1>
    </header>
    <table class="SavedModelsClass">
        <thead>
            <tr>
                <td> Id </td>
                <td> Model Name</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model)
            {
                <tr>
                    <td> @i.id </td>
                    <td><a href="@Url.Action("SetCurrentBucketName", "Guest", new{bucket_name = @i.bucket_name})">
                            @i.model_name </a></td>
                </tr>
            }
        </tbody>
    </table>
    <div class="col-md-12" style="text-align: -webkit-right;">
        <div class="backButton col-md-2">
            <a href="@Url.Action("Index", "Home")">
                <button type="button" class="btn btn-primary btn-block">&#x2190; Back</button>
            </a>
        </div>
    </div>

</div>

<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/2.0.3/css/dataTables.dataTables.css">
<script type="text/javascript" src="https://cdn.datatables.net/2.0.3/js/dataTables.js"></script>

<script type="text/javascript">
    new DataTable('.SavedModelsClass');
</script>
